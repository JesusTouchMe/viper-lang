func @main() -> i32 {
    constexpr x: i32 = 69;

    return x;
}

using struct Vec3 {
    private x: i64;
    private y: i64;
    private z: i64;

    func @addAll() -> i64 {
        return this->getX() + this->getY() + this->getZ();
    }

    func @getX() -> i64 = this->x;

    func @getY() -> i64 = this->y;

    func @getZ() -> i64 {
        return this->z;
    }
}